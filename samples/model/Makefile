  LDFLAGS += `sdl-config --libs` -L ../../bin -L /usr/lib -L /usr/X11R6/lib -L /usr/X11R6/lib -L /usr/lib -lGL -lGLU -lXxf86vm -lz -ljpeg -lSDL_net -lSDL_image -lode -lOPCODE -lfreetype -lftgl -la2e
