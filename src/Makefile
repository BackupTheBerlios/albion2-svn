$(OBJDIR)/%.o : %.cc
	-@if [ ! -d $(OBJDIR) ]; then mkdir -p $(OBJDIR); fi
	@echo $(notdir $<)
	@$(CXX) $(CXXFLAGS) -o $@ -c $<
	-@if [ -f $*.d ]; then mv $*.d $(OBJDIR)/$*.d; fi
	@cp $(OBJDIR)/$*.d $(OBJDIR)/$*.P; \
	 sed -e 's/#.*//' -e 's/^[^:]*: *//' -e 's/ *\\$$//' \
	 -e '/^$$/ d' -e 's/$$/ :/' < $(OBJDIR)/$*.d >> $(OBJDIR)/$*.P; \
	rm -f $(OBJDIR)/$*.d

