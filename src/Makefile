.PHONY: clean

$(OUTDIR)/$(TARGET): $(OBJECTS)
	@echo Linking a2e
	-@if [ ! -d $(BINDIR) ]; then mkdir -p $(BINDIR); fi
	-@if [ ! -d $(LIBDIR) ]; then mkdir -p $(LIBDIR); fi
	@$(CXX) -o $@ $(filter-out %.so, $^) $(LDFLAGS)

clean:
	@echo Cleaning a2e
	-@rm -rf $(OUTDIR)/$(TARGET) $(OBJDIR)/*

-include $(OBJECTS:%.o=%.P)

