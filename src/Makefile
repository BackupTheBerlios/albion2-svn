$(OBJDIR)/%.o : %.s
	-@if [ ! -d $(OBJDIR) ]; then mkdir -p $(OBJDIR); fi
	@echo $(notdir $<)
	@$(CC) -x assembler-with-cpp $(CPPFLAGS) -o $@ -c $<
	-@if [ -f $*.d ]; then mv $*.d $(OBJDIR)/$*.d; fi
