ifndef CONFIG
  CONFIG=Debug
endif

ifeq ($(CONFIG),Debug)
  BINDIR = ..
  LIBDIR = ..
  OBJDIR = obj/Debug
  OUTDIR = ../a2e/lib
  CPPFLAGS = -MD -I "/usr/include" -I "/usr/include/freetype2" -I "/usr/include/FTGL" -I "/usr/include/ode" -I "/usr/include/opcode" -I "/usr/include/opcode/ICE"
  CFLAGS += $(CPPFLAGS) -g `sdl-config --cflags` -Wall
  CXXFLAGS = $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) `sdl-config --libs` -L "/usr/lib" -L "/usr/lib" -L "/usr/lib" -L "/usr/lib" -lGL -lGLU -lXxf86vm -lz -ljpeg -lSDL -lSDL_net -lSDL_image -lode -lOPCODE -lfreetype -lftgl -llua -llualib -lxml2
  LDDEPS =
  TARGET = liba2e.a
endif

