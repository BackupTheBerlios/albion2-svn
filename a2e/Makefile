.PHONY: clean

$(OUTDIR)/$(TARGET): $(OBJECTS) $(LDDEPS)
	@echo Linking a2e
	-@if [ ! -d $(BINDIR) ]; then mkdir -p $(BINDIR); fi
	-@if [ ! -d $(LIBDIR) ]; then mkdir -p $(LIBDIR); fi
	-@if [ ! -d $(OUTDIR) ]; then mkdir -p $(OUTDIR); fi
	@ar -cr $@ $^
	@ranlib $@

clean:
	@echo Cleaning a2e
	-@rm -rf $(OUTDIR)/$(TARGET) $(OBJDIR)

